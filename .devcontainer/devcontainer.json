{
  "name": "VehicleBookingApp - Android (Ubuntu + OpenJDK17)",
  "dockerFile": "Dockerfile",
  "workspaceFolder": "/workspaces/VehicleBookingApp",
  "forwardPorts": [
    8081
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-azuretools.vscode-docker",
        "redhat.java",
        "vscjava.vscode-java-pack",
        "msjsdiag.debugger-for-chrome"
      ]
    }
  },
  "containerEnv": {
    "DEBIAN_FRONTEND": "noninteractive"
  }
}